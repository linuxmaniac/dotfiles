[push]
	default = current
[pull]
	rebase = true
[fetch]
	prune = true
[url "git@salsa.debian.org:"]
	pushInsteadOf = https://salsa.debian.org/
	insteadOf = salsa:
[diff]
	tool = meld
[difftool]
	prompt = false
[difftool "meld"]
	cmd = meld "$LOCAL" "$REMOTE"
[merge]
	tool = meld
[mergetool "meld"]
	cmd = meld "$LOCAL" "$MERGED" "$REMOTE" --output "$MERGED"
[mergetool]
	prompt = false
[commit]
	verbose = true
	#gpgSign = true
[sipwise]
	email = vseva@sipwise.com
	uid = vseva
	gpgSign = false
[help]
	autoCorrect = 2
[gitreview]
	username = vseva
[core]
	excludesfile = /home/vseva/.gitignore_global
